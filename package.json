{
  "name": "fvtt-module-focus-sheet",
  "version": "1.0.0",
  "description": "Foundry VTT Module - Focus Sheet",
  "author": "jagoe",
  "license": "GPL-3.0-or-later",
  "main": "index.js",
  "scripts": {
    "commit": "cz",
    "build": "gulp build",
    "build:dev": "MINIMIZE=false gulp build",
    "lint": "eslint .",
    "lint:staged": "eslint $(git diff --staged --name-status $(git branch --show-current) | grep -P '^(?!D)' | awk '{print $NF}' | grep -E \"(.js$|.ts$)\")",
    "alex:staged": "alex $(git diff --staged --name-status $(git branch --show-current) | grep -P '^(?!D)' | awk '{print $NF}' | grep -E \"(.txt$|.md$)\")",
    "test": "TS_NODE_PROJECT=./test/tsconfig.json mocha --config ./.mocharc.js",
    "test:coverage": "nyc yarn test",
    "test:coverage:lcov": "nyc --reporter=lcov yarn test",
    "postinstall": "is-ci || husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt": "^11.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@league-of-foundry-developers/foundry-vtt-types": "^9.280.0",
    "@types/chai": "^4.2.14",
    "@types/gulp": "^4.0.8",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.10.1",
    "@types/sinon": "^9.0.10",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "alex": "^9.1.0",
    "chai": "^4.2.0",
    "commitizen": "^4.2.5",
    "del": "^6.0.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "generate-json-from-js-webpack-plugin": "^0.1.1",
    "gulp": "^4.0.2",
    "husky": "^5.0.8",
    "is-ci": "^2.0.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "sinon": "^9.2.4",
    "source-map-support": "^0.5.19",
    "ts-loader": "^8.0.14",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "webpack": "^5.75.0"
  },
  "dependencies": {}
}
